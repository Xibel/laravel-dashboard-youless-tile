{
    "name": "xibel/laravel-dashboard-youless-tile",
    "description": "A tile for Laravel-Dashboard that displays statistics from a Youless LS120 energy reader",
    "keywords": [
        "xibel",
        "php",
        "laravel",
        "laravel-dashboard",
        "laravel-dashboard-tile",
        "laravel-dashboard-youless-tile",
        "youless",
        "LS120"
    ],
    "type": "library",
    "require": {
        "php": "^7.4",
        "spatie/laravel-dashboard": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3@dev"
    },
    "autoload": {
        "psr-4": {
            "Xibel\\YoulessTile\\": "src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Xibel",
            "email": "xibel@me.com",
            "homepage": "http://www.xibel-it.eu"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xibel\\YoulessTile\\YoulessTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
}